@{
    ViewData["Title"] = "Editar Contato";
}

<div class="text-center">

    <h1 class="display-4">Editar Contato</h1>

</div>
<form method="post">
     <div class="mb-3">
            <label for="nome">Nome</label>
            <input type="text" class="form-control" placeholder="Digite o nome do Empresa">
        </div>
         <div class="mb-3">
            <label for="Cnpj">CNPJ</label>
            <input type="number" class="form-control" placeholder="Digite o CNPJ da empresa">
        </div>

         <div class="mb-3">
            <label for="email">E-mail</label>
            <input type="text" class="form-control" placeholder="Digite o E-mail do contato">
        </div>

         <div class="mb-3">
            <label for="Celular">Celular</label>
            <input type="tel" class="form-control" placeholder="Digite o número do contato">
        </div>

         <div class="mb-3">
            <label for="InscricaoEstudal">Inscrição Estadual</label>
            <input type="text" class="form-control" placeholder="Digite a Inscrição Estadual">
        </div>


    <button type="submit" class="btn btn-primary ">Alterar</button>
    <a class="btn btn-secondary " asp-controller="Contato" asp-action="Index" role="button">Voltar</a>

</form>



